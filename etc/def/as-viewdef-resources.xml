	<view name="Resources" title="Resources">
		<sections>
			<section title="Text" type="module">
				<module>ASResourceData</module>
				<document>TextResource</document>
				<filter name="AZ" field="name"/>
				<elements>
					<element type="field" name="id" comparable="true">
	        		   	<decorator name="idToA">
	        		 		<rule>3</rule>
	        		   	</decorator>
					</element>
					<element type="field" name="name" comparable="default"> 
	        		   	<decorator name="editLink"/>
					</element>
					<element type="field" name="value" comparable="true"> 
	        		   	<decorator name="editLink"/>
					</element>
	        		<element type="function" name="edit"/>
	        		<element type="function" name="duplicate"/>
	        		<element type="function" name="delete"/>
	      		</elements>
	      		<dialog name="Edit" title="Edit TextResource">
        			<elements>
	       				<element type="empty"/>
		       			<element type="field" name="id" readonly="true"/>
		      			<element type="field" name="name" /> 
		      			<element type="field" name="value" /> 
						<element type="empty"/>
						<element type="list">
							<element type="function" name="update"/>
							<element type="function" name="cancel"/>
						</element>
        			</elements>
      			</dialog>
	    	</section>
	    	
	    	<section title="Country" type="module">
				<module>ASResourceData</module>
				<document>CountryResource</document>
				<filter name="AZ" field="name"/>
				<elements>
					<element type="field" name="id" comparable="true">
	        		   	<decorator name="idToA">
	        		 		<rule>3</rule>
	        		   	</decorator>
					</element>
					<element type="field" name="name" comparable="default"> 
						<decorator name="editLink"/>
					</element>
	        		<element type="function" name="edit"/>
	        		<element type="function" name="duplicate"/>
	        		<element type="function" name="delete"/>
	      		</elements>
	      		<dialog name="Edit" title="Edit CountryResource">
        			<elements>
	       				<element type="empty"/>
		       			<element type="field" name="id" readonly="true"/>
		      			<element type="field" name="name" /> 
						<element type="empty"/>
						<element type="list">
							<element type="function" name="update"/>
							<element type="function" name="cancel"/>
						</element>
        			</elements>
      			</dialog>
	    	</section>
	    	
	    	<section title="State" type="module">
				<module>ASResourceData</module>
				<document>StateResource</document>
				<filter name="AZ" field="name"/>
				<elements>
					<element type="field" name="id" comparable="true">
	        		   	<decorator name="idToA">
	        		 		<rule>3</rule>
	        		   	</decorator>
					</element>
					<element type="field" name="name" comparable="default"> 
						<decorator name="editLink"/>
					</element>
					<element type="field" name="country" comparable="true"/>
	        		<element type="function" name="edit"/>
	        		<element type="function" name="duplicate"/>
	        		<element type="function" name="delete"/>
	      		</elements>
	      		<dialog name="Edit" title="Edit StateResource">
        			<elements>
	       				<element type="empty"/>
		       			<element type="field" name="id" readonly="true"/>
		      			<element type="field" name="name" />
		      			<element type="field" name="country" /> 
						<element type="empty"/>
						<element type="list">
							<element type="function" name="update"/>
							<element type="function" name="cancel"/>
						</element>
        			</elements>
      			</dialog>
	    	</section>
	    	
		</sections>
	</view>
