  <module name="ASGeographicData">
	<document name="Country">
		<property type="string" name="name"/>
		<property type="string" name="code"/>
		<property type="string" name="description" multilingual="true"/>
		<property name="priority" type="enumeration" enumeration="Priority"/>
	</document>
	<document name="State">
		<property type="string" name="name"/>
		<property type="string" name="description" multilingual="true"/>
		<link name="country" type="single" target="ASGeographicData.Country"/>
		<property name="priority" type="enumeration" enumeration="Priority"/>
	</document>
	<document name="City">
		<property type="string" name="name"/>
		<property type="string" name="description" multilingual="true"/>
		<link name="state" type="single" target="ASGeographicData.State"/>
		<property name="priority" type="enumeration" enumeration="Priority"/>
	</document>
	<document name="Language">
		<property type="string" name="name"/>
		<property type="string" name="code"/>
		<property type="string" name="description" multilingual="true"/>
		<property name="priority" type="enumeration" enumeration="Priority"/>
	</document>		
	<document name="ZipCodeRegion">
		<property type="string" name="zipCode"/>
		<link name="country" type="single" target="ASGeographicData.Country"/>
		<property type="float" name="latitude"/>
		<property type="float" name="longitude"/>
	</document>
  </module>